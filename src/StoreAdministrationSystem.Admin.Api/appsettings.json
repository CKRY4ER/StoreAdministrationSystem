{
  "Integrations": {
    "StoreAdministrationSystem": {
      "Address": "http://localhost"
    }
  },
  "Serilog": {
    "Using": [
      "Serilog.AspNetCore",
      "Serilog.Enrichers.Environment",
      "Serilog.Enrichers.Context",
      "Serilog.Enrichers.Thread",
      "Serilog.Enrichers.Span",
      "Serilog.Enrichers.Sensitive",
      "Serilog.Expressions",
      "Serilog.Exceptions",
      "Serilog.Sinks.Async",
      "Serilog.Sinks.Console",
      "Serilog.Sinks.Seq",
      "Serilog.Formatting.Compact"
    ],
    "Filter": [],
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "Microsoft.Hosting.Lifetime": "Information",
        "Microsoft.AspNetCore.HttpLogging.HttpLoggingMiddleware": "Information",
        "KycService": "Information"
      }
    },
    "WriteTo": [
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId",
      "WithProperty",
      "WithSensitiveDataMasking",
      "WithSpan",
      "WithExceptionDetails"
    ],
    "Properties": {
      "Application": "StoreAdministrationSystem.Admin.Api"
    }
  },
  "Cors": {
    "Name": "SpecificOrigins",
    "Origins": [
      "http://localhost:8082"
    ]
  },
  "AllowedHosts": "*"
}
